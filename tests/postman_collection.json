		{
			"name": "Create a new dental clinic",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "d5e34082-02d8-409d-8816-21a6d1ca8b8d",
						"exec": [
							"var jsonData = pm.response.json()",
							"postman.setEnvironmentVariable('dentalClinicId', jsonData._id)",
							"",
							"pm.test('Response is CREATED.', function() { pm.response.to.have.status(201) })",
							"",
							"pm.test('Response should have valid JSON data.', function() {",
							"    pm.expect(jsonData.email).to.eql('admin@hellodental.se')",
							"})"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"email\": \"admin@hellodental.se\",\n    \"password\": \"superSecure\",\n    \"name\": \"Hello Dental\"\n}"
				},
